{
  "name": "c4ddev",
  "version": "0.1.4",
  "author": "Niklas Rosenstein <rosensteinniklas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-loader": "cli build-loader -c -o c4ddev-loader.pyp"
  },
  "bin": {
    "c4ddev": "cli"
  },
  "dependencies": {
    "nodepy-standalone-builder": "~0.0.1"
  },
  "python-dependencies": {
    "click": ">=6.7",
    "nr": ">=1.4.3"
  },
  "dist": {
    "exclude_files": [
      "Makefile",
      "Craftfile",
      "build/*",
      "craftr/*",
      ".pubfile",
      "loader.pyp",
      "docs/*",
      "plugins/*",
      "res/*",
      "src/*",
      "apex.pyp",
      "*.cdl64",
      "*.exp",
      "*.lib",
      "*.dylib"
    ]
  }
}
