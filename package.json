{
  "name": "@NiklasRosenstein/c4ddev",
  "version": "0.1.5",
  "author": "Niklas Rosenstein <rosensteinniklas@gmail.com>",
  "license": "MIT",
  "description": "swiss army knife for Cinema 4D plugin development",
  "repository": "https://github.com/NiklasRosenstein/c4ddev/",
  "scripts": {
    "build-loader": "cli build-loader -c -o c4ddev-loader.pyp"
  },
  "bin": {
    "c4ddev": "cli"
  },
  "dependencies": {
    "@nodepy/standalone-builder": "~0.0.5"
  },
  "python-dependencies": {
    "bs4": "",
    "click": ">=6.7",
    "nr": ">=1.4.3"
  },
  "dist": {
    "exclude_files": [
      "*.cdl64",
      "*.exp",
      "*.lib",
      "*.dylib",
      "build/*",
      "craftr/*",
      "docs/*",
      "extras/*",
      "plugins/*",
      "res/*",
      "src/*",
      ".pubfile",
      "Craftfile",
      "Makefile",
      "c4ddev.pyp",
      "loader.pyp",
      "logo.afdesign"
    ]
  }
}